Config { commands = [ Run Weather "KORD" [ "-t", "<tempF>F, <rh>%, <skyCondition>, <visibility>"
                                         , "-L", "55", "-H", "75"
                                         , "--normal", "white", "--high", "red", "--low", "lightblue"] 18000
                    , Run StdinReader
                    , Run Com "hostname" ["-s"] "" 36000
                    , Run Date "%Y-%m-%d %H:%M" "date" 100 ]
       , template = "%StdinReader% }{ <fc=#4444ff>%hostname%</fc> | %KORD% | <fc=#666666>%date%</fc>"

