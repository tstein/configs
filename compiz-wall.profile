[shift]
s0_initiate_key = Disabled
s0_terminate_button = Disabled
s0_speed = 2.400000
s0_shift_speed = 1.340000
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300
s0_mipmaps = true

[extrawm]
s0_toggle_fullscreen_key = <Control>F11

[resizeinfo]
s0_gradient_1 = #cccce6cc
s0_gradient_2 = #f3f3f3cc
s0_gradient_3 = #d9d9d9cc

[fadedesktop]
s0_fadetime = 250

[wall]
s0_miniscreen = true
s0_slide_duration = 0.250000
s0_mmmode = 1
s0_edgeflip_dnd = true

[decor]
s0_command = gtk-window-decorator --replace

[maximumize]
s0_trigger_max_key = <Super>q
s0_trigger_max_vertically = <Shift><Super>q

[crashhandler]
s0_start_wm = true
s0_wm_cmd = xfwm4 --replace

[fade]
s0_window_match = (any)

[ezoom]
s0_zoom_in_button = Disabled
s0_zoom_out_button = Disabled
s0_zoom_specific_1_key = <Super>grave
s0_spec_target_focus = false
s0_fit_to_window_key = Disabled
s0_zoom_mode = 1
s0_follow_focus = false
s0_speed = 32.000000

[grid]
s0_put_maximize_key = Disabled
s0_put_restore_key = Disabled

[scaleaddon]
s0_close_key = <Control><Super>x
s0_pull_key = <Control><Super>c
s0_highlight_color = #ffffff36

[opengl]
s0_texture_filter = 2
s0_texture_compression = true

[scale]
s0_initiate_edge = 
s0_initiate_key = <Super>e
s0_initiate_all_key = <Alt><Super>e
s0_show_desktop = false

[core]
s0_active_plugins = core;composite;crashhandler;opengl;compiztoolbox;maximumize;copytex;decor;resize;regex;grid;gnomecompat;place;workarounds;neg;winrules;annotate;commands;shift;titleinfo;move;extrawm;mousepoll;session;wall;wobbly;scale;expo;ezoom;scaleaddon;switcher;bench;
s0_hsize = 3
s0_vsize = 3

[neg]
s0_screen_toggle_key = Disabled
s0_exclude_match = 

[winrules]
s0_above_match = name=Docky

[expo]
s0_expo_key = <Control><Super>e
s0_expo_edge = 
s0_zoom_time = 0.350000
s0_deform = 2
s0_vp_distance = 0.000000
s0_curve = 0.200000
s0_mipmaps = true
s0_multioutput_mode = 1
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[group]
s0_glow_size = 18

[commands]
s0_command0 = compiz-gtk --replace
s0_command1 = mouse-switchscreen -s 0
s0_command2 = mouse-switchscreen -s 1
s0_command3 = google-chrome --incognito
s0_command4 = terminator -bml htop
s0_command5 = nautilus --no-desktop
s0_command6 = xscreensaver-command -lock
s0_command7 = xfce4-session-logout
s0_run_command0_key = <Alt><Super>p
s0_run_command1_key = <Super>1
s0_run_command2_key = <Super>2
s0_run_command3_key = <Shift><Control>n
s0_run_command4_key = <Alt><Super>h
s0_run_command5_key = <Alt><Super>b
s0_run_command6_key = <Alt>l
s0_run_command7_key = <Alt>Delete

[switcher]
s0_speed = 2.500000
s0_zoom = 0.000000
s0_auto_rotate = true

